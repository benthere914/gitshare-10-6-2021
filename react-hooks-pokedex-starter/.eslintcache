d5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/components/PokemonBrowser.js",["55"],"import { useEffect, useState } from 'react';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { NavLink, Route, useParams } from 'react-router-dom';\nimport { getPokemon } from '../store/pokemon';\nimport PokemonDetail from './PokemonDetail';\nimport CreatePokemonForm from './CreatePokemonForm';\nimport Fab from './Fab';\n\nconst PokemonBrowser = () => {\n  const { pokemonId } = useParams();\n  const pokemon = useSelector(state => {\n    return state.pokemon.list.map(pokemonId => state.pokemon[pokemonId]);\n  });\n  const [showForm, setShowForm] = useState(false);\n\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(getPokemon());\n\n  },[])\n  if (!pokemon) {\n    return null;\n  }\n\n\n  return (\n    <main>\n      <nav>\n        <Fab hidden={showForm} onClick={() => setShowForm(true)} />\n        {pokemon.map((pokemon) => {\n          return (\n            <NavLink key={pokemon.name} to={`/pokemon/${pokemon.id}`}>\n              <div\n                className={\n                  Number.parseInt(pokemonId) === pokemon.id\n                    ? \"nav-entry is-selected\"\n                    : \"nav-entry\"\n                }\n              >\n                <div\n                  className=\"nav-entry-image\"\n                  style={{ backgroundImage: `url('${pokemon.imageUrl}')` }}\n                ></div>\n                <div>\n                  <div className=\"primary-text\">{pokemon.name}</div>\n                  <div className=\"secondary-text\">\n                    {pokemon.no} {pokemon.captured && \"(Captured)\"}\n                  </div>\n                </div>\n              </div>\n            </NavLink>\n          );\n        })}\n      </nav>\n      {showForm ? (\n        <CreatePokemonForm hideForm={() => setShowForm(false)} />\n      ) : (\n        <Route path=\"/pokemon/:pokemonId\">\n          <PokemonDetail/>\n        </Route>\n      )}\n    </main>\n  );\n};\n\nexport default PokemonBrowser;\n","/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/store/items.js",["56","57","58"],"/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/store/pokemon.js",[],"/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/components/Fab.js",[],"/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/components/PokemonDetail.js",["59"],"import { useState, useEffect } from 'react';\nimport { useSelector, useDispatch } from 'react-redux';\nimport { useParams } from 'react-router-dom';\nimport PokemonItems from './PokemonItems';\nimport EditPokemonForm from './EditPokemonForm';\nimport EditItemForm from './EditItemForm';\nimport { getPokemonDetails } from '../store/pokemon';\n\n\nconst PokemonDetail = () => {\n  const { pokemonId } = useParams();\n  const pokemon = useSelector(state => state.pokemon[pokemonId]);\n  const [showEditPokeForm, setShowEditPokeForm] = useState(false);\n  const [editItemId, setEditItemId] = useState(null);\n  \n  const dispatch = useDispatch()\n\n  useEffect(() => {\n    setShowEditPokeForm(false);\n    setEditItemId(null);\n    dispatch(getPokemonDetails(pokemonId))\n  }, [pokemonId]);\n\n  if (!pokemon || !pokemon.moves) {\n    return null;\n  }\n\n  let content = null;\n\n  if (editItemId) {\n    content = (\n      <EditItemForm pokemon={pokemon} itemId={editItemId} hideForm={() => setEditItemId(null)} />\n    )\n  } else if (showEditPokeForm && pokemon.captured) {\n    content = (\n      <EditPokemonForm pokemon={pokemon} hideForm={() => setShowEditPokeForm(false)} />\n    )\n  } else {\n    content = (\n      <div className=\"pokemon-detail-lists\">\n        <div>\n          <h2>Information</h2>\n          <ul>\n            <li>\n              <b>No</b> {pokemon.no}\n            </li>\n            <li>\n              <b>Type</b> {pokemon.type}\n            </li>\n            <li>\n              <b>Attack</b> {pokemon.attack}\n            </li>\n            <li>\n              <b>Defense</b> {pokemon.defense}\n            </li>\n            <li>\n              <b>Moves</b>\n              <ul>\n                {pokemon.moves && pokemon.moves.map((move) => (\n                  <li key={move}>{move}</li>\n                ))}\n              </ul>\n            </li>\n          </ul>\n        </div>\n        <div>\n          <h2>Items</h2>\n          <table>\n            <thead>\n              <tr>\n                <th></th>\n                <th>Name</th>\n                <th>Happiness</th>\n                <th>Price</th>\n              </tr>\n            </thead>\n            <tbody>\n              <PokemonItems pokemon={pokemon} setEditItemId={setEditItemId} />\n            </tbody>\n          </table>\n        </div>\n      </div>\n    );\n  }\n\n  return (\n    <div className=\"pokemon-detail\">\n      <div className={`pokemon-detail-image-background`}>\n        <div\n          className=\"pokemon-detail-image\"\n          style={{ backgroundImage: `url('${pokemon.imageUrl}')` }}\n        ></div>\n        <div>\n          <h1 className=\"bigger\">{pokemon.name}</h1>\n          {(!showEditPokeForm && pokemon.captured) && (\n            <button onClick={() => setShowEditPokeForm(true)}>Edit</button>\n          )}\n        </div>\n\n      </div>\n      {content}\n    </div>\n  );\n};\n\nexport default PokemonDetail;\n","/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/components/CreatePokemonForm.js",[],"/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/components/PokemonItems.js",["60"],"/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/components/EditPokemonForm.js",[],"/home/nick/mod5/2Week/gitshare-10-6-2021/react-hooks-pokedex-starter/src/components/EditItemForm.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"